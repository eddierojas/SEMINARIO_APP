<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FA4753">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#FA4753"
        android:text="CATEGORIAS"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.037" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="357dp"
        android:layout_height="602dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <androidx.cardview.widget.CardView
                    android:layout_width="156dp"
                    android:layout_height="130dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="10dp"
                    android:backgroundTint="@color/white"
                    app:cardCornerRadius="10dp">

                    <ImageButton
                        android:id="@+id/imgbtn_calzado"
                        android:layout_width="139dp"
                        android:layout_height="120dp"
                        android:layout_marginHorizontal="10dp"
                        android:layout_marginTop="5dp"
                        android:backgroundTint="@color/white"
                        android:onClick="categoriaZapato"
                        android:visibility="visible"
                        app:srcCompat="@mipmap/iconosapato"
                        tools:ignore="SpeakableTextPresentCheck" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="156dp"
                    android:layout_height="130dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="10dp"
                    android:backgroundTint="@color/white"
                    app:cardCornerRadius="10dp">

                    <ImageButton
                        android:id="@+id/imageButton5"
                        android:onClick="categoriaRopaje"
                        android:layout_width="139dp"
                        android:layout_height="120dp"
                        android:layout_marginHorizontal="10dp"
                        android:layout_marginTop="5dp"
                        android:backgroundTint="@color/white"
                        app:srcCompat="@mipmap/iconoropa"
                        tools:ignore="SpeakableTextPresentCheck" />
                </androidx.cardview.widget.CardView>
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <androidx.cardview.widget.CardView
                    android:layout_width="156dp"
                    android:layout_height="130dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="10dp"
                    android:backgroundTint="@color/white"
                    app:cardCornerRadius="10dp">

                    <ImageButton
                        android:id="@+id/imageButton6"
                        android:onClick="categoriaCosmeticos"
                        android:layout_width="139dp"
                        android:layout_height="120dp"
                        android:layout_marginHorizontal="10dp"
                        android:layout_marginTop="5dp"
                        android:backgroundTint="@color/white"
                        app:srcCompat="@mipmap/iconocosmetico"
                        tools:ignore="SpeakableTextPresentCheck" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="156dp"
                    android:layout_height="130dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="10dp"
                    android:backgroundTint="@color/white"
                    app:cardCornerRadius="10dp">

                    <ImageButton
                        android:id="@+id/imageButton8"
                        android:onClick="categoriaPlatos"
                        android:layout_width="139dp"
                        android:layout_height="120dp"
                        android:layout_marginHorizontal="10dp"
                        android:layout_marginTop="5dp"
                        android:backgroundTint="@color/white"
                        app:srcCompat="@mipmap/iconocmida"
                        tools:ignore="SpeakableTextPresentCheck" />
                </androidx.cardview.widget.CardView>
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <androidx.cardview.widget.CardView
                    android:layout_width="156dp"
                    android:layout_height="130dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="10dp"
                    android:backgroundTint="@color/white"
                    app:cardCornerRadius="10dp">

                    <ImageButton
                        android:id="@+id/imageButton7"
                        android:onClick="categoriaCupcakes"
                        android:layout_width="139dp"
                        android:layout_height="120sp"
                        android:layout_marginHorizontal="10dp"
                        android:layout_marginTop="5dp"
                        android:backgroundTint="@color/white"
                        app:srcCompat="@mipmap/iconocup"
                        tools:ignore="SpeakableTextPresentCheck" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="156dp"
                    android:layout_height="130dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="10dp"
                    android:backgroundTint="@color/white"
                    app:cardCornerRadius="10dp">

                    <ImageButton
                        android:id="@+id/imageButton9"
                        android:onClick="categoriaLicor"
                        android:layout_width="139dp"
                        android:layout_height="120dp"
                        android:layout_marginHorizontal="10dp"
                        android:layout_marginTop="5dp"
                        android:backgroundTint="@color/white"
                        app:srcCompat="@mipmap/iconolicor"
                        tools:ignore="SpeakableTextPresentCheck" />
                </androidx.cardview.widget.CardView>
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <androidx.cardview.widget.CardView
                    android:layout_width="156dp"
                    android:layout_height="130dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="10dp"
                    android:backgroundTint="@color/white"
                    app:cardCornerRadius="10dp">

                    <ImageButton
                        android:id="@+id/imageButton10"
                        android:onClick="categoriaVidrio"
                        android:layout_width="130sp"
                        android:layout_height="120sp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginHorizontal="10dp"
                        android:layout_marginTop="5dp"
                        android:backgroundTint="@color/white"
                        app:srcCompat="@mipmap/iconocubierto"
                        tools:ignore="SpeakableTextPresentCheck" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="156dp"
                    android:layout_height="130dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="10dp"
                    android:backgroundTint="@color/white"
                    app:cardCornerRadius="10dp">

                    <ImageButton
                        android:id="@+id/imageButton11"
                        android:onClick="categoriaJuego"
                        android:layout_width="139dp"
                        android:layout_height="120dp"
                        android:layout_marginHorizontal="10dp"
                        android:layout_marginTop="5dp"
                        android:backgroundTint="@color/white"
                        app:srcCompat="@mipmap/icnogame"
                        tools:ignore="SpeakableTextPresentCheck" />
                </androidx.cardview.widget.CardView>
            </TableRow>
        </TableLayout>
    </ScrollView>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:backgroundTint="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/scrollView2"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>